                <div class="col-md-10" style="margin-left: -8px">
                    <div class="table-responsive">
                        <table class="table-hover table table-bordered container-fluid">
                            <thead style="background: royalblue; color: whitesmoke;">


                                <tr class="center-text">

                                    <td colspan="12" class="text-center">
                                        <div class="center-element">
                                            <h5 class="normalCase">NORMAL CASE</h5>
                                        </div>
                                    </td>

                                </tr>

                            </thead>
                            <tbody>
                                <tr style="background: royalblue; color: whitesmoke">
                                    <td colspan="2" class="td04">Type Of Visa</td>
                                    <td colspan="2" class="td14">Type Of Visa</td>
                                    <td colspan="2" class="td24">Type Of Visa</td>
                                    <td colspan="2" class="td34">Type Of Visa</td>
                                    <td colspan="2" class="td44">Type Of Visa</td>

                                    <td colspan="2" class="td54">STAMPING FEE</td>
                                    <td id="idtd03" class="hide"></td>
                                </tr>
                                @{
                                    var g = ViewBag.Tourist as List<ViSaPart1>;


                                    if (g != null)
                                    {
                                        int i = 0;
                                        foreach (var w in g)
                                        {
                                            var t1 = w.pax1;
                                            var t2 = w.pax2;
                                            var t3 = w.pax3;
                                            var t4 = w.pax4;
                                            if (t1 > 0 && t2 > 0 && t3 > 0 && t4 > 0)
                                            {
                                                <tr class="trTOURIST">
                                                    <td colspan="2" rowspan="1" class="td01">@w.typeOfVisa</td>
                                                    <td colspan="2" rowspan="1" class="td11">@t1&nbsp;USD</td>
                                                    <td colspan="2" rowspan="1" class="td21">@t2&nbsp;USD</td>
                                                    <td colspan="2" rowspan="1" class="td31">@t3&nbsp;USD</td>
                                                    <td colspan="2" rowspan="1" class="td41">@t4&nbsp;USD</td>


                                                    <td colspan="2" rowspan="1" class="td51">@w.valueP&nbsp;USD</td>
                                                    <td colspan="2" class="hidden tdid">@w.id&nbsp;$</td>
                                                </tr>
                                            }

                                            if (t1 <= 0 && t2 <= 0 && t3 <= 0 && t4 <= 0)
                                            {
                                                <tr class="trTOURIST">
                                                    <td colspan="2" class="td01">@w.typeOfVisa</td>
                                                    <td colspan="10" class="text-center">N/A</td>


                                                </tr>
                                            }

                                        }

                                    }

                                }


                            </tbody>


                        </table>
                    </div>
                    <div>
                        @{
                            var ga = ViewBag.feeT as List<Processingtime>;
                            if (ga != null)
                            {
                                foreach (var w in ga)
                                {
                                    <div class="panel panel-info">
                                        <div class="panel-heading" style="background: royalblue; color: whitesmoke;">
                                            <h2 class="panel-title text-center"><span>@w.name &nbsp;@w.noteName</span></h2>
                                        </div>
                                        <div class="panel-body text-center text-danger">
                                            Plus  @w.value USD/pax
                                        </div>
                                    </div>
                                }
                            }
                        }

                    </div>

                </div>

            </div>

