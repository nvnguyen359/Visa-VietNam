<script >
    
    $(document)
        .ready(function() {
          
            loadHeading();

        })
</script>
<script>
    function loadHeading() {
    
        var url = '@Url.Action("LoadHeading", "Default")';
        $.ajax({
            url: url,
            datatype: "JSON",
            type: "POST",
            success: function(data) {
               
                $(".td04").text(data.name);
                $(".td14").text(data.pax1);
                $(".td24").text(data.pax2);
                $(".td34").text(data.pax3);
                $(".td44").text(data.pax4);
                $(".idtd03").text(data.id);
            }
        })
    }
</script>