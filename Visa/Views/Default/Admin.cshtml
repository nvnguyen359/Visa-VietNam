<div class="container-fluid" style="margin-top: 60px">
  
    <div class="row">
        <div class="col-md-4" id="Nationality">


            @{
                var dsqt = ViewBag.quoctich as List<Nationality>;
                if (dsqt != null)
                {
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title" id="dsqtcount">Toatl Nationality:&nbsp; @dsqt.Count</h3>
                        </div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label class="control-label" for="tbn-Nationality"> Nationality</label>
                                <input list="listItem" class="form-control" id="tbn-Nationality" name="nationality[]" style="height: 35px;" title="Select" />
                                <datalist id="listItem">
                                    <option disabled="" selected="">Select</option>
                                    @{
                                        foreach (var f in dsqt)
                                        {

                                            <option>@f.Name</option>
                                        }
                                    }
                                </datalist>
                            </div>
                            <div class="form-group label-floating">
                                <label class="control-label" for="values">Value</label>
                                <input class="form-control" id="values" type="text">
                            </div>

                            <div class="row">
                                <button class="btn btn-inverse text-center" onclick=" AddNationality() ">Add</button>
                                <button class="btn btn-inverse text-center" onclick=" DeleteAddNationality() ">Delete</button>
                            </div>
                        </div>
                    </div>


                                        }
            }

        </div>
        <div class="col-md-4" id="arri">


            @{
                var sanbay = ViewBag.sanbay as List<TArrivalPort>;
                if (sanbay != null)
                {
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title" id="dsqtcountsb">Total Arriva lPort:&nbsp; @sanbay.Count</h3>
                        </div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label class="control-label" for="tbn-Nationality"> Arrival Port</label>
                                <input list="listItem1" class="form-control" id="tbn-sanbay" name="sanbay[]" style="height: 35px;" title="Select"/>
                                <datalist id="listItem1">
                                    <option disabled="" selected="">Select</option>
                                    @{
                                        foreach (var f in sanbay)
                                        {

                                            <option>@f.name</option>
                                        }
                                    }
                                </datalist>
                            </div>
                            @*<div class="form-group label-floating">
                                <label class="control-label" for="valuessanbay">Value</label>
                                <input class="form-control" id="valuessanbay" type="text">
                            </div>*@

                            <div class="row">
                                <button class="btn btn-inverse text-center" onclick="AddSanbay() ">Add</button>
                                <button class="btn btn-inverse text-center" onclick="DeleteSanbay() ">Delete</button>
                            </div>
                        </div>
                    </div>


                }
            }

        </div>
        <div class="col-md-4" id="sex">


            @{
                var sex = ViewBag.sex as List<Gender>;
                if (sex != null)
                {
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title">Total Gender:&nbsp; @sex.Count</h3>
                        </div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label class="control-label" for="tbn-Nationality"> Gender</label>
                                <input list="listItem2s" class="form-control" id="tbn-sex" name="sex[]" style="height: 35px;" title="Select"/>
                                <datalist id="listItem2s">
                                    <option disabled="" selected="">Select</option>
                                    @{
                                        foreach (var f in sex)
                                        {

                                            <option>@f.Name</option>
                                        }
                                    }
                                </datalist>
                            </div>
                            @*<div class="form-group label-floating">
                                <label class="control-label" for="valuessanbay">Value</label>
                                <input class="form-control" id="valuessanbay" type="text">
                            </div>*@

                            <div class="row">
                                <button class="btn btn-inverse text-center" onclick=" AddSex() ">Add</button>
                                <button class="btn btn-inverse text-center" onclick=" DeleteSex() ">Delete</button>
                            </div>
                        </div>
                    </div>


                }
            }

        </div>
        <div class="col-md-4" id="GiaPhong">

            @{
                var giaphong = ViewBag.giaphong as List<TGiaPhong>;
                if (giaphong != null)
                {
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title">Total Price :&nbsp; @giaphong.Count</h3>
                        </div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label class="control-label" for="tbn-Nationality"> Price</label>
                                <input list="listItem3s" class="form-control" onblur="OnChangePrice(this)" id="tbn-giaphong" name="sex[]" style="height: 35px;" title="Select"/>
                                <datalist id="listItem3s">
                                    <option disabled="" selected="">Select</option>
                                    @{
                                        foreach (var f in giaphong)
                                        {
                                            if (@f.den == 0 || f.den == null)
                                            {
                                    <option>@f.ten &nbsp;@f.tu@f.donvitien </option>}
                                            else
                                            {
                                                <option>@f.ten &nbsp;@f.tu@f.donvitien &rarr;@f.den@f.donvitien</option>
                                            }

                                        }
                                    }
                                </datalist>
                            </div>
                            <div class="form-group label-floating">
                                <label class="control-label" for="from">From</label>
                                <input class="form-control" id="from" type="number">
                            </div>
                            <div class="form-group label-floating">
                                <label class="control-label" for="to">To</label>
                                <input class="form-control" id="to" type="number">
                            </div>
                            <div class="form-group label-floating">
                                <label class="control-label" for="dv">Don vi</label>
                                <input class="form-control" id="dv" type="text">
                            </div>
                            <div class="row">
                                <button class="btn btn-inverse text-center" onclick=" AddGiaPhong() ">Add</button>
                                <button class="btn btn-inverse text-center" onclick=" DeleteGiaPhong() ">Delete</button>
                            </div>
                        </div>
                    </div>


                }
            }


        </div>
    <div class="col-md-4" id="DiaDiem">

        @{
            var dd = ViewBag.diadiem as List<TLocaltion>;
            if (dd != null)
            {
                <div class="panel panel-primary">
