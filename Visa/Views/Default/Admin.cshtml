                            <datalist id="listItem4s">
                                <option disabled="" selected="">Select</option>
                                @{
                                    foreach (var f in dd)
                                    {
                                        <option>@f.name</option>

                                    }
                                }
                            </datalist>
                        </div>

                        <div class="row">
                            <button class="btn btn-inverse text-center" onclick=" AddDiadiem() ">Add</button>
                            <button class="btn btn-inverse text-center" onclick=" DeleteDiadiem() ">Delete</button>
                        </div>
                    </div>
                </div>


            }
        }


    </div>
  
        <div class="col-md-4" id="romtype">

            @{
                var rom = ViewBag.rom as List<TRomtype>;
                if (rom != null)
                {
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title">Total Rom :&nbsp; @rom.Count</h3>
                        </div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label class="control-label" for="tbn-rom"> Rom type</label>
                                <input list="listItem5s" class="form-control" id="tbn-rom" name="rom[]" style="height: 35px;" title="Select" />
                                <datalist id="listItem5s">
                                    <option disabled="" selected="">Select</option>
                                    @{
                                        foreach (var f in rom)
                                        {
                                            <option>@f.tenphong</option>

                                        }
                                    }
                                </datalist>
                            </div>

                            <div class="row">
                                <button class="btn btn-inverse text-center" onclick="AddRom() ">Add</button>
                                <button class="btn btn-inverse text-center" onclick="DeleteRom() ">Delete</button>
                            </div>
                        </div>
                    </div>


                                        }
            }


        </div>
        <div class="col-md-4" id="Numberofapplicant">

            @{
                var numberofvisa = ViewBag.numberofvisa as List<TNumberofapplicant>;
                if (numberofvisa != null)
                {
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title">Total Number Of Visa :&nbsp; @numberofvisa.Count</h3>
                        </div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label class="control-label" for="tbn-rom"> Number Of Visa</label>
                                <input list="listItem7s" onblur="OnselectNumberOfVisa()" class="form-control" id="tbn-numberofvisa" name="numberofvisa[]" style="height: 35px;" title="Select" />
                                <datalist id="listItem7s">
                                    <option disabled="" selected="">Select</option>
                                    @{
                                        foreach (var f in numberofvisa)
                                        {
                                            <option>@f.valuenumber &nbsp;@f.valuetext</option>

                                        }
                                    }
                                </datalist>
                            </div>
                            <div class="row">
                                <div class="form-group label-floating">
                                    <label class="control-label" for="valueText">Value Text</label>
                                    <input class="form-control" id="valueText" type="text">
                                </div>
                                <div class="form-group label-floating">
                                    <label class="control-label" for="valueNumber">Value Number</label>
                                    <input class="form-control" id="valueNumber" type="text">
                                </div>
                            </div>
                            <div class="row">
                                <button class="btn btn-inverse text-center" onclick="Addnumberofvisa()">Add</button>
                                <button class="btn btn-inverse text-center" onclick="DeleteNumberOfVisa()">Delete</button>
                            </div>
                        </div>
                    </div>


                                        }
            }


        </div>
        <div class="col-md-4" style="height: 400px; background: wheat"></div>
    </div>
</div>
